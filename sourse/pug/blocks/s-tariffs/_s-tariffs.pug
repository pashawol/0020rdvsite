mixin s-tariffs()
	// start s-tariffs
	+b.SECTION.s-tariffs.section#s-tariffs&attributes(attributes)
		.container
			+e.wrapper
				+e.item-wrapper
					+e.item
						+e.item-title.bg-primary Стартап
						+e.UL.list
							+e.LI.list-item 
								+e.item-text <strong>Цена подписки в месяц / при оплате за год</strong>
								+e.item-price <span>7 990 ₽</span>
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 6 мес
								+e.item-price 8 490 ₽
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 3 мес
								+e.item-price 8 990 ₽
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 1 мес
								+e.item-price 9 990 ₽
							+e.LI.list-item 
								+e.item-text Макс. количество одновременно работающих пользователей
								+e.item-price до 5
				+e.item-wrapper
					+e.item 
						+e.item-title профи
						+e.UL.list
							+e.LI.list-item 
								+e.item-text <strong>Цена подписки в месяц / при оплате за год</strong>
								+e.item-price <span>11 990 ₽</span>
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 6 мес
								+e.item-price 12 740 ₽
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 3 мес
								+e.item-price 13 490 ₽
							+e.LI.list-item 
								+e.item-text Цена подписки в месяц / при оплате за 1 мес
								+e.item-price 14 990 ₽
							+e.LI.list-item 
								+e.item-text Макс. количество одновременно работающих пользователей
								+e.item-price до 5
				+e.item-wrapper
					+e.item
						+e.item-title.bg-warning.color Серверная установка
						+e.UL.list.server-price
							+e.LI.list-item 
								+e.item-text <strong>Стоимость программного продукта</strong>
								+e.item-price <span>49 900 ₽</span>
							+e.LI.list-item 
								+e.item-text Цена подписки  на сопровождение / в месяц
								+e.item-price 9 990 ₽
							+e.LI.list-item 
								+e.item-text Макс. количество одновременно работающих пользователей
								+e.item-price не ограничено
			+e.btn-wrapper
				+e.A.btn.btn-primary(href="#") подобрать тариф
					span.btn-icon-wrap
						+icon("chevron-right2") 

	// end s-tariffs

mixin s-market-tariffs()
	// start s-market-tariffs
	+b.SECTION.s-tariffs.section#s-tariffs&attributes(attributes)
		.container
			+b.section-title
				h2 Работу с маркетплейсами эффективной
			.row
				+e.radio-box.col-auto
					span.h5 При оплате за:
						+in-c("radio","1 мес.","tarrifs")
						+in-c("radio","3 мес.","tarrifs")
						+in-c("radio","6 мес.","tarrifs")
						+in-c("radio","1 год","tarrifs")
			+e.wrapper
				+e.item-wrapper
					+e.item
						+e.item-title.bg-primary Стартап
						+e.UL.list
							+e.LI.list-item 
								+e.item-text <strong>Стоимость подписки в месяц:</strong>
								+e.item-price
									+e.pre-price.h6 9 990 ₽
									<span>8 880 ₽</span>
							+e.LI.list-item 
								+e.item-text Итого за период:
								+e.item-price 18 000 ₽
							+e.LI.list-item 
								+e.item-text Количество пользователей:
								+e.item-price до 5
							+e.LI.list-item 
								+e.A.btn(href="#") Выбрать
									span.btn-icon-wrap
										+icon("chevron-right2") 
				+e.item-wrapper
					+e.item 
						+e.item-title профи
						+e.UL.list
							+e.LI.list-item 
								+e.item-text <strong>Стоимость подписки в месяц:</strong>
								+e.item-price
									+e.pre-price.h6 9 990 ₽
									<span>8 880 ₽</span>
							+e.LI.list-item 
								+e.item-text Итого за период:
								+e.item-price 18 000 ₽
							+e.LI.list-item 
								+e.item-text Количество пользователей:
								+e.item-price до 10
							+e.LI.list-item 
								+e.A.btn(href="#") Выбрать
									span.btn-icon-wrap
										+icon("chevron-right2") 
				+e.item-wrapper
					+e.item
						+e.item-title.bg-warning.color Серверная установка
						+e.UL.list
							+e.LI.list-item 
								+e.item-text <strong>Стоимость серверного сопровождения:</strong>
								+e.item-price <span>49 900 ₽</span>
							+e.LI.list-item 
								+e.item-text Подписка на сопровождение в месяц:
								+e.item-price 9 990 ₽
							+e.LI.list-item 
								+e.item-text Количество пользователей:
								+e.item-price до 5
							+e.LI.list-item 
								+e.A.btn(href="#") Выбрать
									span.btn-icon-wrap
										+icon("chevron-right2")
			+e.btn-wrapper
				+e.A.btn.btn-primary(href="#") детальные условия
					span.btn-icon-wrap
						+icon("chevron-right2")  

	// end s-market-tariffs